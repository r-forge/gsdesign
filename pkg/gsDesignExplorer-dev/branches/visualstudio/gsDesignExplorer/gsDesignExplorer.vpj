			<F N="src/gsDesignGUI.cpp"/>
			<F N="src/gsDesignTips.cpp"/>
			<F N="src/GsRList.cpp"/>
			<F N="src/main.cpp"/>
			<F N="src/Rcpp.cpp"/>
		</Folder>
		<Folder
			Name="Header Files"
			Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.inc;*.sh;*.cpy;*.if">
			<F N="src/gsdesign.h"/>
			<F N="src/gsDesignGUI.h"/>
			<F N="src/GsRList.h"/>
		</Folder>
		<Folder
			Name="Resource Files"
			Filters="*.ico;*.cur;*.dlg"/>
